{
  "name": "query-overview",
  "version": "1.1.1",
  "description": "Interactive bar chart for exploration of query data from clinical trials",
  "main": "./build/queryOverview.js",
  "author": "Rho, Inc.",
  "license": "MIT",
  "dependencies": {
    "d3": "~3",
    "webcharts": "~1.7"
  },
  "scripts": {
    "build": "rollup -c && npm run format",
    "format": "npm run format-src && npm run format-build",
        "format-build": "prettier --print-width=100 --tab-width=4 --single-quote --write ./build/queryOverview.js",
        "format-src": "prettier --print-width=100 --tab-width=4 --single-quote --write \"src/**/*.js\"",
    "watch": "rollup -c -w"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-watch": "~3.2.2",
    "prettier":"~1"
  }
}
